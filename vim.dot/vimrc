" dein.vimで管理してるpluginを読み込む
source ~/.vimrc.plugin
" 基本設定
source ~/.vimrc.basic
" StatusLine設定
source ~/.vimrc.statusline
" インデント設定
source ~/.vimrc.indent
" 表示関連
source ~/.vimrc.apperance
" 補完関連
source ~/.vimrc.completion
" 検索関連
source ~/.vimrc.search
" 移動関連
source ~/.vimrc.moving
" Color関連
source ~/.vimrc.colors
" 編集関連
source ~/.vimrc.editing
" エンコーディング関連
source ~/.vimrc.encoding
" プラグインに依存するアレ
source ~/.vimrc.plugins_setting
" その他
source ~/.vimrc.misc

# Configuration file for use in Zsh
# ---------------------------------------------------**
. ~/.tmux/tmuxinator.zsh
. ~/.zsh/zsh-bd/bd.zsh
. ~/.zshrc.env
. ~/.zshrc.config
. ~/.zshrc.setting
. ~/.zshrc.color
. ~/.zshrc.alias
. ~/.zshrc.function
. ~/.zshrc.local

# When OSX or Linux
# ---------------------------------------------------**
case "${OSTYPE}" in
  darwin*)
    . ~/.zshrc.osx
    ;;

  linux*)
    . ~/.zshrc.linux
    ;;
esac
